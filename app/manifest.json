{
  "_version": "1.1.0",
  "sap.app": {
    "id": "myapp",
    "type": "application",
    "i18n": "locales/locale.properties",
    "title": "{{myapp.title}}",
    "description": "{{myapp.description}}",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "offline": true
  },
  "sap.ui": {
    "_version": "1.1.0",
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": false
    },
    "supportedThemes": [
      "sap_bluecrystal"
    ]
  },
  "sap.ui5": {
    "_version": "1.1.0",
    "resources": {
      "css": [
        {
          "uri": "css/style.css",
          "id": "customstyle"
        }
      ]
    },
    "models": {
      "locale": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "myapp.locales.locale"
        }
      }
    },
    "dependencies": {
      "minUI5Version": "1.30.0",
      "libs": {
        "sap.m": {
          "minVersion": "1.30.0"
        },
        "sap.ui.commons": {
          "minVersion": "1.30.0"
        }
      }
    },
    "rootView": "myapp.view.App",
	"routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewPath": "myapp",
        "controlId": "rootControl",
        "controlAggregation": "pages",
        "viewType": "XML"
      },
      "routes": [
        {
          "name": "main",
          "pattern": "",
          "target": "main"
        },
        {
          "name": "tmp",
          "pattern": "tmp",
          "target": "tmp"
        }
      ],
      "targets": {
        "main": {
          "viewName": "view.Main",
          "viewLevel": 0
        },
        "tmp": {
          "viewName": "view.Tmp",
          "viewLevel": 1
        }
      }
    },
    "config": {
      "managersLocal": "/model/products.json",
      "managersRemote": "/sap/opu/odata/sap/ZPPM_GW_SCRUM_SRV"
    }
  }
}
